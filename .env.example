# Enhanced COD Studio with MCP Integration - Environment Configuration

# Fireworks API Configuration
FIREWORKS_API_KEY=your_fireworks_api_key_here

# Next.js Configuration
NEXT_PUBLIC_APP_NAME="Enhanced COD Studio with MCP"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# MCP Configuration
MCP_VERBOSE_LOGS=true
MCP_MAX_DURATION=60

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_DEBUG_MODE=false

# Optional: Redis Configuration (for production memory storage)
# REDIS_URL=redis://localhost:6379
# REDIS_PASSWORD=your_redis_password

# Optional: Database Configuration (for persistent memory)
# DATABASE_URL=postgresql://user:password@localhost:5432/enhanced_cod_studio
